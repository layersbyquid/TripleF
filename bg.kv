<GetStarted>
    canvas.before:
        Color:
            rgba: (147/255,190/255,230/255,1)
        Rectangle:
            pos: self.pos
            size: self.size

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        padding: 50
        spacing: 20

        Image:
            source: 'logo.png'

        Button:
            background_color: (0/255, 255/255, 255/255, 1)
            text: "Get Started"
            pos_hint: {'center_x': 0.5}
            size_hint: (1, .3)
            font_size: 50
            font_name: "Boogaloo-Regular"
            on_press: 
                root.manager.current = 'li'
            # background_color: (0/255,151/255,167/255,1)
            # backgorund_normal: ''
        


