<Cardio>
    canvas.before:
        Color:
            rgba: (147/255,190/255,230/255,1)
        Rectangle:
            pos: self.pos
            size: self.size
    
        Color:
            rgba: (2/255,110/255,122/255,1)
        Rectangle:
            pos: (45, 720)
            size: (550, 150)

    Label:
        id: timer_label
        text: '00:00'
        font_name: 'Boogaloo-Regular.ttf'
        font_size: 130
        pos: (5, 230)

    BoxLayout:
        orientation: "vertical"
        pos: (120, 200)
        pos: 0, root.height - 950
        spacing: 50

        Button:
            background_color: (0/255, 255/255, 255/255, 1)
            text: "Start"
            pos_hint: {'center_x': 0.5, 'center_y' : 0.5}
            size_hint: (.8, None)
            font_size: 50
            font_name: "Boogaloo-Regular"
            on_press: root.start_timer()

        Button:
            background_color: (0/255, 255/255, 255/255, 1)
            text: "Pause"   
            pos_hint: {'center_x': 0.5, 'center_y' : 0.5}
            size_hint: (.8, None)
            font_size: 50
            font_name: "Boogaloo-Regular"
            on_press: root.pause_timer()

        Button:
            text: "Reset"
            background_color: (0/255, 255/255, 255/255, 1)
            pos_hint: {'center_x': 0.5, 'center_y' : 0.5}
            size_hint: (.8, None)
            font_size: 50
            font_name: "Boogaloo-Regular"
            on_press: root.reset_timer()


