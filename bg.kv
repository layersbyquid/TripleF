<DailyGoal>:
    canvas.before:
        Color:
            rgba: (37/255,41/255,38/255,1)
        Rectangle:
            pos: self.pos
            size: self.size

    Image:
        source: 'pfp.png'
        size_hint: (.15, .15)
        pos_hint: {'right': 0.98, 'top' : 1}

    Button:
        size_hint: (.15, .15)
        pos_hint: {'right': 0.98, 'top' : 1}
        background_color: 0,0,0,0
        on_press:
            root.manager.current = 's'

    Image:
        source: 'home.png'
        size_hint: (.15, .15)
        pos_hint: {'right': 0.18, 'top' : 1}

    Button:
        size_hint: (.15, .15)
        pos_hint: {'right': 0.18, 'top' : 1}
        background_color: 0,0,0,0
        on_press:
            root.manager.current = 'home'

    BoxLayout:
        orientation: "vertical"
        pos: 0, root.height - 700
        spacing: 50

        Label:
            text: "How many calories would you like to burn today?"
            font_size: 30
            size_hint_y: None
            height: self.texture_size[1]
            font_name: "Boogaloo-Regular.ttf"

        NumericTextInput:
            id: calories_input
            hint_text: "Enter calories"
            font_size: 30
            size_hint_y: None
            height: self.minimum_height
            size_hint: (.8, 0)
            pos_hint: {'center_x': 0.5, 'center_y' : 0.5}
            multiline: False
            font_name: "Boogaloo-Regular.ttf"

        Button:
            text: "Submit"
            size_hint: (None, None)
            size: (150, 75)
            pos_hint: {'center_x': 0.5}
            on_press: 
                root.on_submit()
            background_color: (0/255, 255/255, 255/255, 1)
            font_name: "Boogaloo-Regular.ttf"
            font_size: 30

        Button:
            text: "Change Goal"
            id: enable_button
            size_hint: (None, None)
            size: (150, 75)
            pos_hint: {'center_x': 0.5}
            on_press:
                root.enable_input()
            background_color: (0/255, 255/255, 255/255, 1)
            font_name: "Boogaloo-Regular.ttf"
            font_size: 30
            

